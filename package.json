{
  "name": "react-jsbox-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "npm run build; parcel watch src/main.js --no-source-maps --no-hmr & jsbox watch dist",
    "clean": "rimraf dist .cache",
    "build": "npm run clean; parcel build src/main.js --no-source-maps && jsbox build dist",
    "format": "prettier --ignore-path .gitignore --single-quote --write src/**/*.{js,md}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "iOS 11"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "iOS 11"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "url-loader": {
    "exts": [
      "jpg",
      "jpeg",
      "png",
      "gif"
    ],
    "limit": 10240
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.4",
    "babel-eslint": "^10.0.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "eslint-plugin-jsbox": "^0.1.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jsbox-cli": "^1.2.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-static-files-copy": "^2.2.0",
    "parcel-plugin-url-loader": "^1.3.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "escape-goat": "^2.1.0",
    "htm": "^2.2.1",
    "immer": "^3.1.3",
    "invert-color": "^2.0.0",
    "react": "alpha",
    "react-jsbox": "^0.0.43",
    "use-immer": "^0.3.3"
  },
  "staticFiles": {
    "staticPath": [
      "src"
    ],
    "watcherGlob": "**"
  }
}
