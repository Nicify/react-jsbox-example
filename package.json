{
  "name": "react-jsbox-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "run-p bundle watch",
    "watch": "jsbox watch dist",
    "bundle": "parcel watch src/main.js --no-source-maps --no-hmr",
    "clean": "rimraf .cache",
    "build": "npm run clean && parcel build src/main.js --no-source-maps",
    "build:box": "jsbox build dist",
    "format": "prettier --ignore-path .gitignore --single-quote --write src/**/*.{js,md}",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "iOS 12"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "iOS 12"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "url-loader": {
    "exts": [
      "jpg",
      "jpeg",
      "png",
      "gif"
    ],
    "limit": 10240
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "eslint-plugin-jsbox": "^0.1.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jsbox-cli": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-static-files-copy": "^2.4.1",
    "parcel-plugin-url-loader": "^1.3.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "htm": "^3.0.4",
    "immer": "^6.0.9",
    "invert-color": "^2.0.0",
    "pupa": "github:nicify/pupa#master",
    "react": "^16.13.1",
    "react-jsbox": "^1.0.6",
    "use-immer": "^0.4.0",
    "use-simple-store": "^1.3.0"
  },
  "staticFiles": {
    "staticPath": [
      "src"
    ],
    "watcherGlob": "^**"
  }
}
