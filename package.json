{
  "name": "react-jsbox-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "run-p bundle watch",
    "watch": "jsbox watch dist",
    "bundle": "parcel watch src/main.js --no-source-maps --no-hmr",
    "clean": "rimraf .cache",
    "build": "npm run clean && parcel build src/main.js --no-source-maps",
    "build:box": "jsbox build dist",
    "format": "prettier --ignore-path .gitignore --single-quote --write src/**/*.{js,md}",
    "commit": "git-cz",
    "release": "standard-version",
    "postinstall": "npm run build"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "iOS 11"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "iOS 11"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "url-loader": {
    "exts": [
      "jpg",
      "jpeg",
      "png",
      "gif"
    ],
    "limit": 10240
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint-plugin-jsbox": "^0.1.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jsbox-cli": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "parcel-plugin-url-loader": "^1.3.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1"
  },
  "dependencies": {
    "core-js": "^3.4.2",
    "escape-goat": "^2.1.1",
    "htm": "^2.2.1",
    "immer": "^5.0.0",
    "invert-color": "^2.0.0",
    "mobx": "^5.15.0",
    "mobx-react-lite": "^1.5.1",
    "pupa": "github:nicify/pupa#master",
    "react": "^16.12.0",
    "react-jsbox": "0.0.63",
    "use-immer": "^0.3.5"
  },
  "staticFiles": {
    "staticPath": [
      "src"
    ],
    "watcherGlob": "^**"
  }
}