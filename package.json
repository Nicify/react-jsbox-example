{
  "name": "react-jsbox-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "parcel src/main.js --no-source-maps",
    "clean": "rimraf dist",
    "build": "npm run clean && parcel build src/main.js --no-source-maps && jsbox build dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "iOS 11"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false,
          "targets": "iOS 11",
          "exclude": [
            "transform-regenerator"
          ]
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "eslint-plugin-jsbox": "^0.1.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jsbox-cli": "^1.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "react": "alpha",
    "react-jsbox": "^0.0.33",
    "react-router": "^5.0.1"
  },
  "staticFiles": {
    "staticPath": [
      "src"
    ],
    "watcherGlob": "**"
  }
}