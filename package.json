{
  "name": "react-jsbox-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "run-p bundle watch",
    "watch": "jsbox watch dist",
    "bundle": "parcel watch src/main.js --no-source-maps --no-hmr",
    "clean": "rimraf .cache",
    "build": "npm run clean && parcel build src/main.js --no-source-maps",
    "build:box": "jsbox build dist",
    "lint": "eslint --fix src/**/*.js",
    "format": "prettier --ignore-path .gitignore --single-quote --write src/**/*.{js,md}",
    "postinstall": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && git add ."
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "iOS 12"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "iOS 12"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "url-loader": {
    "exts": [
      "jpg",
      "jpeg",
      "png",
      "gif"
    ],
    "limit": 10240
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/runtime": "7.12.5",
    "babel-eslint": "10.1.0",
    "eslint": "7.15.0",
    "eslint-plugin-jsbox": "0.1.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "jsbox-cli": "1.2.1",
    "npm-run-all": "4.1.5",
    "parcel": "1.12.4",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "parcel-plugin-nuke-dist": "1.0.1",
    "parcel-plugin-static-files-copy": "2.5.0",
    "parcel-plugin-url-loader": "1.3.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "core-js": "^3.8.1",
    "htm": "^3.0.4",
    "immer": "^8.0.0",
    "invert-color": "^2.0.0",
    "lodash-es": "^4.17.15",
    "pupa": "github:nicify/pupa#master",
    "react": "^17.0.1",
    "react-jsbox": "^1.3.0",
    "swr": "^0.4.1",
    "use-immer": "^0.4.2",
    "use-simple-store": "^1.4.0"
  },
  "staticFiles": {
    "staticPath": [
      "src"
    ],
    "watcherGlob": "^**"
  }
}
