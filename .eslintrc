{
  "extends": ["plugin:jsbox/default"],
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "globals": {
    "JSBox": "writable"
  },
  "rules": {
    "semi": [2, "never"],
    "arrow-parens": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0
  }
}
